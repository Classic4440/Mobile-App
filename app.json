{
  "expo": {
    "name": "HotspotManager",
    "slug": "hotspotmanager",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "scheme": "hotspotmanager",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#001A17"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hotspotmanager.app",
      "newArchEnabled": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#001A17"
      },
      "package": "com.hotspotmanager.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "784b1b39-d328-4df3-a8c7-4d47d749bfb8"
      }
    }
  }
}
